  delay(300);
  noTone(12);

  delay(50);
  
  tone(12,330);
  delay(300);
  noTone(12);

  delay(50);
  
  tone(12,330);
  delay(300);
  noTone(12);

  delay(50);
  
  tone(12,330);
  delay(300);
  noTone(12);

  delay(50);
  
  tone(12,262);
  delay(300);
  noTone(12);

  delay(50);
  
  tone(12,262);
  delay(300);
  noTone(12);

  delay(50);
  
  tone(12, 262);
  delay(300);
  noTone(12);

  delay(50);
  
  tone(12,392);
  delay(600);
  noTone(12);

  delay(50);
  
  tone(12,349);
  delay(300);
  noTone(12);

  delay(50);
  
  tone(12,330);
